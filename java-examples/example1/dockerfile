FROM openjdk:8-jdk-alpine
VOLUME /tmp
COPY build/libs/example1-1.0-SNAPSHOT.jar app.jar
ENTRYPOINT ["java","-jar","/app.jar","Main1"]

ARG MONGODBURI
ARG MONGODBNAME
ARG AUTORIZACIONES_PATH
ARG AUTORIZACIONES_URL
ARG TIPO_DOCS_PATH
ARG TIPO_DOCS_URL
ARG AGENDA_PATH
ARG AGENDA_URL
ARG POLIZA_DIG_PATH
ARG POLIZA_DIG_URL
	
ENV MONGODBURI=${MONGODBURI} \
	MONGODBNAME=${MONGODBNAME} \
	AUTORIZACIONES_PATH=${AUTORIZACIONES_PATH} \
	AUTORIZACIONES_URL=${AUTORIZACIONES_URL} \
	TIPO_DOCS_PATH=${TIPO_DOCS_PATH} \
	TIPO_DOCS_URL=${TIPO_DOCS_URL} \	
	AGENDA_PATH=${AGENDA_PATH} \
	AGENDA_URL=${AGENDA_URL} \	
	POLIZA_DIG_PATH=${POLIZA_DIG_PATH} \
	POLIZA_DIG_URL=${POLIZA_DIG_URL}
	
	
	
	